@model VHS.Services.Models.PropertyAdditionalInfoModel
<h3>Additonal Information</h3>
<p class="sub-heading"><span class="error-msg">some error occured</span></p>
<div class="col-lg-6 col-md-6">
    <form class="form-part" method="post" action="profileform.php" name="profileform" id="profileform" autocomplete="off">
        <label>Add Property Description<span class="required">*</span></label>
        <textarea name="description" id="description"></textarea>
        <label class="sm-label">Check-in time<span class="required">*</span></label>
        <input name="check-in" class="sm-input" type="text" id="" size="12" title="" />
        <label class="sm-label">Check-out time<span class="required">*</span></label>
        <input name="check-out" class="sm-input" type="text" id="" size="12" title="" />
        <label class="sm-label">Size of Property<br />(/sq. ft)<span class="required">*</span></label>
        <input name="rooms" class="sm-input" type="text" id="rooms" size="12" title="" />
        <label class="sm-label">Persons per room<span class="required">*</span></label>
        <input name="bathrooms" class="sm-input" type="text" id="bathrooms" size="12" title="" />
        <div style="clear:both;"></div>
        <label class="sm-label">Pets allowed<span class="required">*</span></label>
        <select class="sm-input">
            <option value="0">select</option>
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
        <div style="clear:both;"></div>
        <label class="sm-label">Drinking allowed<span class="required">*</span></label>
        <select class="sm-input">
            <option value="0">select</option>
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
        <div style="clear:both;"></div>
        <label class="sm-label">Smoking Allowed<span class="required">*</span></label>
        <select class="sm-input">
            <option value="0">select</option>
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
        <div style="clear:both;"></div>
        <label class="sm-label">Family/Kid Friendly<span class="required">*</span></label>
        <select class="sm-input">
            <option value="0">select</option>
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
        <div style="clear:both;"></div>
        <label class="sm-label">Wheelchair Accessible<span class="required">*</span></label>
        <select class="sm-input">
            <option value="0">select</option>
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
        <div style="clear:both;"></div>
        <label class="full-label">Parking<span class="required">*</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>On Premises</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Open Parking</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Covered Parking</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Paid Parking</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Valet</span></label>
        <div style="clear:both;"></div>
        <label>Amenities</label>
        <label class="full-label">Sleeping Arrangment</label>
        <label class="half-label"><input type="checkbox" name=""><span>Beds & Mattresses</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Cupboard</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Personal Safe</span></label>
        <div style="clear:both;"></div>
        <label class="full-label">Bathrooms</label>
        @*<label class="half-label"><input type="checkbox" name=""><span>Hot Tub</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Water Supply</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Hot Water Supply</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Bathtub</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Hair Dryer</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Shower</span></label>
            <label class="half-label"><input type="checkbox" name=""><span>Towels</span></label>*@
        @if (Model != null && Model.BathRoomsList != null)
            {
                foreach (var bathRoom in Model.BathRoomsList)
                {
                <label class="half-label"><input type="checkbox" checked="@bathRoom.IsChecked" name="TravelPreferencesId" value="@bathRoom.id"><span>@bathRoom.Name</span></label>
            }
        }
        <label class="full-label">Kitchen</label>
        <label class="half-label"><input type="checkbox" name=""><span>Gas</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Crockery And Cutlery</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Coffee Maker</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Dish Washer</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Fridge</span></label>
