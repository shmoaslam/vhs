        <label class="half-label"><input type="checkbox" name=""><span>First Aid Kit</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Dental Kit</span></label>
        <label class="full-label">Entertainment And Electronics</label>
        <label class="half-label"><input type="checkbox" name=""><span>Internet</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>WiFi</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>TV</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Music System</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Scanner/Printer</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Phone(Booth/Lines)</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Projector</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Fax</span></label>
