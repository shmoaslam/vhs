        <label class="half-label"><input type="checkbox" name=""><span>AC</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Heating</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Furniture</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Dryer</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Washing Machine</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Security</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Butler</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Cook</span></label>
        <label class="half-label"><input type="checkbox" name=""><span>Doorman</span></label>
