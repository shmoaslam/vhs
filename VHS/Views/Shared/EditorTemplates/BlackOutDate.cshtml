@model  VHS.Services.Models.BlakOutDate
@using (Html.BeginCollectionItem("BlackOutDatsList"))
{
    @Html.HiddenFor(m => m.BlackOutDateId)
    <label class="editor">From</label>
    @Html.TextBoxFor(m => m.StartDate, new { @class = "inputEditor", @type = "date", @style = "border:1px solid #d8d8d8;background-color:#f5f5f5;width:100%;padding:4px 18px 16px 18px" })
    <span style="color:red"> @Html.ValidationMessageFor(m => m.StartDate)</span><br />
    <label class="editor">To</label>
        @Html.TextBoxFor(m => m.EndDate, new { @class = "inputEditor", @type = "date", @style = "border:1px solid #d8d8d8;background-color:#f5f5f5;width:100%;padding:4px 18px 16px 18px" })
        <span style="color:red"> @Html.ValidationMessageFor(m => m.EndDate)</span><br />
}
